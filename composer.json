{
  "name": "lumenity/framework",
  "version": "3.8.0",
  "description": "This is a simple PHP framework for building web applications.",
  "keywords": [
    "framework",
    "php",
    "lumenity",
    "web",
    "application"
  ],
  "type": "project",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Lumenity\\Framework\\": "."
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lumenity\\Framework\\": "test"
    }
  },
  "authors": [
    {
      "name": "Febri Ananda Lubis",
      "email": "febryananda17@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.2",
    "illuminate/database": "^11.2",
    "illuminate/http": "^11.2",
    "illuminate/collections": "^11.2",
    "vlucas/phpdotenv": "^5.6",
    "eftec/bladeone": "^4.13",
    "filp/whoops": "^2.15",
    "monolog/monolog": "3.5.0",
    "symfony/validator": "^7.0",
    "rakit/console": "^0.1.3",
    "ramsey/uuid": "^3.9"
  },
  "require-dev": {
    "phpunit/phpunit": "9.5.26"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
