{
  "name": "lumenity/framework",
  "version": "5.0.0",
  "description": "This is a simple PHP framework for building web applications.",
  "keywords": [
    "framework",
    "php",
    "lumenity",
    "web",
    "application"
  ],
  "type": "project",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Lumenity\\Framework\\": "."
    },
    "files": [
      "config/common/function/app.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Lumenity\\Framework\\": "."
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ]
  },
  "authors": [
    {
      "name": "Febri Ananda Lubis",
      "email": "febryananda17@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.2",
    "eftec/bladeone": "^4.13",
    "filp/whoops": "^2.15",
    "monolog/monolog": "3.5.0",
    "ramsey/uuid": "^3.9",
    "symfony/validator": "^7.1",
    "vlucas/phpdotenv": "^5.6",
    "illuminate/database": "^11.20",
    "illuminate/http": "^11.20",
    "illuminate/collections": "^11.20",
    "illuminate/container": "^11.20"
  },
  "require-dev": {
    "phpunit/phpunit": "9.5.26",
    "rakit/console": "^0.1.3",
    "robmorgan/phinx": "^0.16.2",
    "symfony/yaml": "^7.1",
    "fakerphp/faker": "^1.23"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
